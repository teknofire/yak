<OGRVRTDataSource>
    <OGRVRTLayer name="<%= name %>">
        <SrcDataSource><%= name %>.csv</SrcDataSource>
        <GeometryType>wkbPoint</GeometryType>
        <LayerSRS>WGS84</LayerSRS>
        <GeometryField encoding="PointFromColumns" x="lon" y="lat"/>
    </OGRVRTLayer>
</OGRVRTDataSource>
